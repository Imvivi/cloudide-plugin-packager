{
  "name": "@cloudide/plugin-packager",
  "version": "0.0.1",
  "description": "A packager tool for huawei-cloudide's plugin.",
  "bin": {
    "cloudide-pack": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "license": "SEE LICENSE IN LICENSE.txt",
  "scripts": {
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "build": "tsc && npm run format-code",
    "format-code": "tsfmt -r",
    "watch": "tsc --watch"
  },
  "dependencies": {
    "archiver": "^4.0.1",
    "commander": "^2.15.1",
    "fs-extra": "^9.0.0",
    "glob-promise": "^3.4.0",
    "micromatch": "^4.0.2",
    "ora": "^4.0.4",
    "read-pkg": "^5.2.0"
  },
  "keywords": [
    "huawei",
    "cloudide",
    "plugin",
    "pack"
  ],
  "devDependencies": {
    "@types/archiver": "^3.1.0",
    "@types/fs-extra": "^9.0.1",
    "@types/micromatch": "^4.0.1",
    "typescript": "^3.9.3",
    "rimraf": "^3.0.2",
    "tslint": "^5.1.0",
    "typescript-formatter": "^7.1.0"
  }
}
